package org.tutorial.object;
class Calculator{
	int left, right;
	
	public void setOprands(int left, int right){
		this.left=left;
		this.right=right;
	}
}


public class CalculatorDemo3 {
	public static void main(String[] args) {
		Calculator c1 = new Calculator();
		c1.setOprands(10, 20);
		c1.sum();
		c1.avg();
	}
}
