package org.tutorial.veriable;

public class IntergerDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a;
		a = 1;
		System.out.println(a+1);
		
		a = 2;
		System.out.println(a+1);
				
		double b;
		b = 1.1;
		
		System.out.println(b+1.1);
		
		String first;
		first = "coding";
		
		
	}

}
